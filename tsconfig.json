{
  "compilerOptions": {
    "target": "es2019",
    "jsx": "react",
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "dom"
    ],
    "baseUrl": ".",
    "sourceMap": true,
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "removeComments": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true,
  },
  "exclude": [
    "node_modules"
  ]
}
