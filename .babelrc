{
  "presets": [
    [
      "@babel/preset-env",
      { "targets": { "ie": "11" }, "modules": false }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-transform-object-assign",
    [ "@babel/plugin-transform-for-of", { "loose": true } ],
    "react-hot-loader/babel"
  ]
}
